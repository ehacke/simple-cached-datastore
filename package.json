{
  "name": "simple-cached-datastore",
  "description": "Datastore wrapper with simplified API and optional caching built in",
  "version": "1.0.1",
  "author": "Eric Hacke",
  "bugs": {
    "url": "https://github.com/ehacke/simple-cached-datastore/issues"
  },
  "dependencies": {
    "@ehacke/redis": "^2.0.7",
    "@types/bluebird": "^3.5.42",
    "@types/ioredis": "^5.0.0",
    "@types/lodash": "^4.17.16",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.14.0",
    "@types/pino": "^7.0.5",
    "@types/redlock": "^4.0.7",
    "bluebird": "^3.7.2",
    "clean-deep": "^3.4.0",
    "err": "^2.1.13",
    "fast-json-stable-stringify": "^2.1.0",
    "flat": "^6.0.1",
    "lodash": "^4.17.21",
    "luxon": "^3.6.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "redlock": "^4.2.0",
    "ts-essentials": "^10.0.4"
  },
  "devDependencies": {
    "@ehacke/commitlint-config": "^4.0.2",
    "@ehacke/eslint-config": "^2.1.2",
    "@ehacke/prettier-config": "^3.0.1",
    "@google-cloud/datastore": "^9.2.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai-as-promised": "^8.0.2",
    "@types/expect": "^24.3.2",
    "@types/getenv": "^1.0.3",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "axios": "^1.8.4",
    "chai": "^5.2.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.24.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fixpack": "^4.0.0",
    "getenv": "^1.0.0",
    "husky": "^9.1.7",
    "ioredis": "^5.6.0",
    "jsdoc": "^4.0.4",
    "lint-staged": "^15.5.0",
    "mocha": "^11.1.0",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "prettier-eslint-cli": "^8.0.1",
    "sinon": "^20.0.0",
    "ts-sinon": "^2.0.2",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.28.2",
    "typedoc-plugin-markdown": "^4.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "typings": "^2.1.1"
  },
  "files": [
    "dist/**/*.{js,ts,tsbuildinfo}",
    "docs/**/*.*"
  ],
  "homepage": "https://github.com/ehacke/simple-cached-datastore#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "cache",
    "cached",
    "datastore",
    "google"
  ],
  "license": "MIT",
  "lint-staged": {
    "package.json": [
      "fixpack"
    ],
    "**/*.js, **/*.ts": [
      "prettier-eslint --write",
      "eslint --fix"
    ]
  },
  "main": "dist/index.js",
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "datastore.ts"
    ],
    "exclude": [
      "src/scratch.*"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary",
      "lcov"
    ]
  },
  "peerDependencies": {
    "@google-cloud/datastore": "^9.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehacke/simple-cached-datastore.git"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json",
    "docs": "typedoc --plugin typedoc-plugin-markdown --readme none --exclude \"tests/**/*.+(unit|it).+(ts|js)\"",
    "integration": "nyc mocha --recursive -r source-map-support/register -r ts-node/register -r tsconfig-paths/register --use_strict --exit \"tests/**/*.it.+(ts|js)\"",
    "lint": "fixpack && prettier-eslint --write && eslint --fix \"**/*.{js,ts}\"",
    "test": "nyc mocha --recursive -r source-map-support/register -r ts-node/register -r tsconfig-paths/register --use_strict --exit \"tests/**/*.+(unit|it).+(ts|js)\"",
    "unit": "nyc mocha --recursive -r source-map-support/register -r ts-node/register -r tsconfig-paths/register --use_strict --exit \"tests/**/*.unit.+(ts|js)\""
  },
  "types": "dist/index.d.ts"
}
